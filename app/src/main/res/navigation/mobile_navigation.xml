<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.thebookuser.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />


    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.example.thebookuser.navigation_favorite"
        android:label="お気に入り"
        tools:layout="@layout/fragment_navigation_favorite" />
    <fragment
        android:id="@+id/navigation_location"
        android:name="com.example.thebookuser.navigation_location"
        android:label="近くのお店"
        tools:layout="@layout/fragment_navigation_location" />
    <fragment
        android:id="@+id/navigation_choiceaccount"
        android:name="com.example.thebookuser.navigation_choiceaccount"
        android:label="fragment_navigation_choiceaccount"
        tools:layout="@layout/fragment_navigation_choiceaccount" />
    <fragment
        android:id="@+id/navigation_newaccount"
        android:name="com.example.thebookuser.navigation_newaccount"
        android:label="fragment_navigation_newaccount"
        tools:layout="@layout/fragment_navigation_newaccount" />
    <fragment
        android:id="@+id/navigation_repassword"
        android:name="com.example.thebookuser.navigation_repassword"
        android:label="fragment_navigation_repassword"
        tools:layout="@layout/fragment_navigation_repassword" />
    <fragment
        android:id="@+id/navigation_sendmail"
        android:name="com.example.thebookuser.navigation_sendmail"
        android:label="fragment_navigation_sendmail"
        tools:layout="@layout/fragment_navigation_sendmail" />



</navigation>